ui.faces.grateful = "/custom-faces/Mario/GRATEFUL.png"
ui.faces.motivated = "/custom-faces/Mario/MOTIVATED.png"
ui.faces.demotivated = "/custom-faces/Mario/DEMOTIVATED.png"
